{
  "manifest_version": 3,
  "name": "ScrapedKnees",
  "version": "1.0.0",
  "description": "AI-powered web data extraction tool. Train your AI to scrape data from any website with visual training and intelligent pattern recognition.",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "css": ["dist/content.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_title": "ScrapedKnees - AI Data Extraction",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
        "options_page": "dist/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["dist/injected.js"],
      "matches": ["<all_urls>"]
    }
  ]
}