{
  "manifest_version": 3,
  "name": "AI Data Scraper",
  "version": "1.0.0",
  "description": "AI-powered web data scraping with training capabilities",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "css": ["dist/content.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_title": "AI Data Scraper"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["dist/injected.js"],
      "matches": ["<all_urls>"]
    }
  ]
}